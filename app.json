{
  "expo": {
    "name": "my-job",
    "slug": "my-job",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/my-job-icon.png",
    "notification": {
      "icon": "./assets/my-job-icon.png",
      "androidMode": "default",
      "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amirghedira.my-job"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/my-job.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/audio/notification-sound.wav"
          ]
        }
      ]
    ]
  }
}
